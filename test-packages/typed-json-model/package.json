{
  "name": "@ui5/typed-json-model",
  "private": true,
  "version": "1.1.0",
  "description": "Test-driven implementation of a typesafe JSONModel for UI5",
  "author": "Martin Broede",
  "license": "Apache-2.0",
  "scripts": {
    "build:demo": "ui5 build --clean-dest",
    "start": "ui5 serve --port 8080 -o index.html",
    "start:dist": "ui5 serve  --port 8080 -o index.html --config ui5-dist.yaml",
    "ts-typecheck": "tsc --noEmit",
    "lint": "eslint webapp",
    "test": "node ./test_typing.mjs webapp/model/test/cases",
    "ui5lint": "ui5lint",
    "ci": "npm run lint && npm run ui5lint && npm run ts-typecheck && npm run test"
  },
  "devDependencies": {
    "@types/openui5": "1.136.0",
    "@ui5/cli": "^4.0.30",
    "@ui5/linter": "^1.20.2",
    "eslint": "^9.37.0",
    "globals": "^16.4.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.1",
    "ui5-middleware-livereload": "^3",
    "ui5-middleware-simpleproxy": "^3",
    "ui5-tooling-transpile": "^3"
  }
}
